{
  "name": "@crutchcorn/shared-elements",
  "version": "0.0.1",
  "description": "The shared logic between apps in a React Native demo",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test": "jest",
    "dev": "vite build --watch",
    "build": "vite build",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@tanstack/react-query": "^4.20.4",
    "dayjs": "^1.11.7",
    "react": "18.1.0",
    "react-native": "0.70.3",
    "react-native-vector-icons": "^9.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@crutchcorn/config": "workspace:*",
    "@mdx-js/react": "^1.6.22",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.2",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query": "^4.20.4",
    "@testing-library/react-native": "^11.2.0",
    "@types/react-native": "0.70.4",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "18.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^8.3.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "react": "18.1.0",
    "react-dom": "^18.2.0",
    "react-native": "0.70.3",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.18.10",
    "react-test-renderer": "18.1.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.8.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/shared-elements-cjs.js",
  "module": "./dist/shared-elements-es.js",
  "react-native": "./dist/shared-elements-es.js",
  "exports": {
    ".": {
      "import": "./dist/shared-elements-es.js",
      "require": "./dist/shared-elements-cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./polyfills": {
      "import": "./dist/polyfills-es.js",
      "require": "./dist/polyfills-cjs.js",
      "types": "./dist/polyfills.d.ts"
    }
  },
  "dependencies": {
    "@types/uuid": "^9.0.0",
    "uuid": "^9.0.0"
  }
}
