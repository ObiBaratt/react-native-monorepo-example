    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-loader": "^9.1.3",
    "dayjs": "^1.11.9",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.3",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.19.7",
    "react-test-renderer": "18.2.0",
    "storybook": "^7.1.1",
    "styled-components": "^6.0.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.4.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/shared-elements-cjs.js",
  "module": "./dist/shared-elements-es.js",
  "react-native": "./dist/shared-elements-es.js",
  "exports": {
    ".": {
      "import": "./dist/shared-elements-es.js",
      "require": "./dist/shared-elements-cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./polyfills": {
      "import": "./dist/polyfills-es.js",
      "require": "./dist/polyfills-cjs.js",
      "types": "./dist/polyfills.d.ts"
    }
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
